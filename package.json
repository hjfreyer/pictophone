{
  "name": "pictophone",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc": "tsc",
    "dev": "gcloud config set project pictophone-app && FIRESTORE_EMULATOR_HOST=localhost:8181 ts-node-dev --respawn --transpileOnly ./src/index.ts",
    "dev-live": "gcloud config set project pictophone-app && ts-node-dev --respawn --transpileOnly ./src/index.ts",
    "prod": "tsc && node ./dist/index.js",
    "gen": "npm-run-all gen:*",
    "gen:action": "typescript-json-validator src/model/AnyAction.ts ",
    "gen:state": "typescript-json-validator src/model/AnyState.ts ",
    "gen:types": "typescript-json-validator --collection src/types.ts ",
    "gen:rpc": "typescript-json-validator --collection src/model/rpc.ts ",
    "start": "node ./dist/index.js",
    "firestore": "gcloud config set project pictophone-app && gcloud beta emulators firestore start --host-port localhost:8181",
    "sample-data": "ts-node src/sample_data.ts",
    "inspect": "FIRESTORE_EMULATOR_HOST=localhost:8181 ts-node"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.1",
    "@types/express-session": "^1.15.14",
    "@types/node": "^12.7.2",
    "@types/node-fetch": "^2.5.0",
    "@types/request": "^2.48.2",
    "node-fetch": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "ts-node-dev": "^1.0.0-pre.42",
    "tslint": "^5.19.0",
    "typescript": "^3.5.3",
    "typescript-json-schema": "^0.40.0"
  },
  "dependencies": {
    "@google-cloud/storage": "^4.1.2",
    "@types/cors": "^2.8.6",
    "@types/deep-equal": "^1.0.1",
    "@types/uuid": "^3.4.6",
    "cors": "^2.8.5",
    "deep-equal": "^1.1.0",
    "express": "^4.17.1",
    "firebase-admin": "^8.4.0",
    "immer": "^3.2.0",
    "typescript-json-validator": "^2.3.1",
    "uuid": "^3.3.3"
  }
}
